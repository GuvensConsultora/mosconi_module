<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="product_page_template" model="ir.ui.view">
        <field name="name">product.page.template</field>
        <field name="type">qweb</field>
	<field name="key">mosconi.product_page_template</field> <!-- Obligatorio para request.render -->
        <field name="arch" type="xml">
          
                <t t-call="website.layout">
                  <h1><t t-esc="product.name"/></h1>
                        <div class="row">
                            <div class="col-md-4">
                               <t t-call="mosconi.product_template_child"/>
                            </div>
                        </div>
                </t>
          
        </field>
    </record>
</odoo>
